page {
    background-color: #F2F2F2;
}

.top_bg {
    width: 750rpx;
    height: 858rpx;
    background-color: #ccc;
    position: absolute;
    top: 0;
    z-index: -1;
}

.common_box {
    width: 690rpx;
    background-color: #fff;
    margin: 0 auto;
}

.get_price {
    height: 562rpx;
    border-radius: 20rpx;
    padding-top: 78rpx;
}

.common_input_wrap {
    width: 570rpx;
    height: 84rpx;
    border-radius: 42rpx;
    background: #F0EEEF;
    margin: 0 auto 48rpx;
}

.input_one {
    justify-content: space-between;
    font-size: 60rpx;
    line-height: 1;
    color: #333333;
    box-sizing: border-box;
    background: #CCCCCC;
    border: 4rpx solid #999999;
    align-items: center;
}

.input_one .right {
    margin-right: 40rpx;
}

.input_two {
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    padding: 0 40rpx 0 50rpx;
    font-size: 28rpx;
    font-weight: 400;
    line-height: 1;
    color: #CCCCCC;
}

.input_two input {
    color: #333;
}

.get_price_btn {
    width: 570rpx;
    height: 100rpx;
    background: linear-gradient(360deg, #EEB674 0%, #FFDCA5 100%);
    box-shadow: 0 9rpx 21rpx rgba(239, 184, 118, 0.45);
    border-radius: 50rpx;
    font-size: 48rpx;
    font-weight: 500;
    line-height: 100rpx;
    text-align: center;
    color: #663D2A;
    margin: 0 auto;
}

.get_data_title {
    width: 298rpx;
    height: 130rpx;
    display: block;
    margin: 48rpx auto 32rpx;
}

.data_source {
    height: 744rpx;
    padding-top: 72rpx;
}

.data_source_item {
    width: 554rpx;
    font-size: 40rpx;
    font-weight: bold;
    line-height: 1;
    color: #0A0340;
    align-items: center;
    margin: 0 auto 64rpx;
}

.data_source_item image {
    width: 114rpx;
    height: 120rpx;
    margin-right: 40rpx;
    display: block;
}

.plan_box {
    height: 456rpx;
    border-radius: 20rpx;
    background-color: #F2F2F2;
    overflow: hidden;
    justify-content: space-between;
}

.scene_shows {
    border-radius: 20rpx;
    padding: 32rpx 20rpx 1rpx;
    box-sizing: border-box;
}

.plan_item_left, .plan_item_right {
    width: 340rpx;
    height: 456rpx;
    background-color: #fff;
}

.plan_item_left .title{
    font-size: 30rpx;
    line-height: 1;
    color: #0A0340;
    padding-top: 32rpx;
    margin-bottom: 24rpx;
    text-align: center;
}

.scale_wrap {
    font-size: 24rpx;
    line-height: 40rpx;
    text-align: center;
    color: #999999;
    flex-wrap: wrap;
    margin: 0 auto;
    width: 292rpx;
}

.scale_wrap .top_left {
    width: 146rpx;
    height: 146rpx;
    background-color: #FEC100;
    border-radius: 146rpx 0 0 0;
    position: relative;
}

.scale_wrap .name {
    background-color: #fff;
    width: 67rpx;
    height: 40rpx;
}

.top_left .name {
    position: absolute;
    bottom: 32rpx;
    right: 28rpx;
}

.scale_wrap .top_right {
    width: 146rpx;
    height: 146rpx;
    background-color: #5C9BD5;
    border-radius: 0 146rpx 0 0;
    position: relative;
}

.top_right .name {
    position: absolute;
    bottom: 32rpx;
    left: 28rpx;
}

.scale_wrap .bottom_left {
    width: 146rpx;
    height: 146rpx;
    background-color: #A5A5A5;
    border-radius: 0 0 0 146rpx;
    position: relative;
}

.bottom_left .name {
    position: absolute;
    top: 32rpx;
    right: 28rpx;
}

.scale_wrap .bottom_right {
    width: 146rpx;
    height: 146rpx;
    background-color: #EC7E31;
    border-radius: 0 0 146rpx 0;
    position: relative;
}

.bottom_right .name {
    position: absolute;
    top: 32rpx;
    left: 28rpx;
}

.ul {
    font-size: 20rpx;
    color: #999999;
    margin-left: 40rpx;
    margin-top: 24rpx;
}

.ul .li_item {
    line-height: 20rpx;
    align-items: center;
    margin-right: 20rpx;
}

.li_item .icon1 {
    width: 10rpx;
    height: 10rpx;
    background-color: #FEC100;
    margin-right: 5rpx;
}

.li_item .icon2 {
    background-color: #5C9BD5;
}

.li_item .icon3 {
    background-color: #A5A5A5;
}

.li_item .icon2 {
    background-color: #EC7E31;
}

.plan_right_img {
    width: 216rpx;
    height: 192rpx;
    background-color: #ccc;
    display: block;
    margin: 0 auto 51rpx;
}

.plan_right_title {
    font-size: 40rpx;
    font-family: 字魂35号-经典雅黑;
    font-weight: bold;
    text-align: center;
    line-height: 1;
    color: #EC7E31;
}

.plan_item_right .txt {
    font-size: 24rpx;
    font-weight: 600;
    line-height: 35rpx;
    color: #0A0340;
    max-width: 264rpx;
    margin: 14rpx auto;
}

.scene_shows .scene_img {
    width: 337rpx;
    height: 190rpx;
    background: #746666;
    border-radius: 10rpx;
}

.scene_shows .value {
    font-size: 28rpx;
    line-height: 36rpx;
    color: #010101;
    max-width: 280rpx;
    margin-left: 24rpx;
}

.scene_item {
    align-items: center;
    margin-bottom: 32rpx;
}