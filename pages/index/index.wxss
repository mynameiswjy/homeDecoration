.swiper {
  width: 690rpx;
  height: 287rpx;
  border-radius: 10rpx;
  margin: 24rpx 30rpx 0;
}

.swiper_item {
  width: 690rpx;
  height: 287rpx;
  border-radius: 10rpx;
  background-color: #ccc;
}

.menu_wrap {
  margin: 0 30rpx;
  flex-wrap: wrap;
}

.menu_item {
  font-size: 28rpx;
  color: #333333;
  line-height: 1;
  text-align: center;
  width: 156rpx;
  margin-bottom: 8rpx;
}

.menu_img {
  width: 76rpx;
  max-height: 76rpx;
  background-color: #ccc;
  margin-top: 24rpx;
  margin-bottom: 16rpx;
}

.menu_right {
  margin-right: 22rpx;
}

.ad {
  width: 690rpx;
  height: 100rpx;
  border-radius: 20rpx;
  background-color: #ccc;
  margin: 24rpx auto 32rpx;
  display: block;
}

.exclusive {
  padding: 0 30rpx 30rpx;
  font-size: 24rpx;
  color: #333;
  line-height: 1;
}

.exclusive_left {
  width: 337rpx;
  height: 337rpx;
  border-radius: 10rpx;
  background-color: #ccc;
  margin-right: 16rpx;
}

.exclusive_left .txt {
  font-size: 36rpx;
  color: #03BF9D;
  line-height: 1;
  margin: 50rpx 0 16rpx 20rpx;
  font-weight: 500;
}

.exclusive_right .top {
  width: 337rpx;
  height: 160rpx;
  background-color: #ccc;
  margin-bottom: 17rpx;
  border-radius: 10rpx;
}

.exclusive_right .bottom {
  width: 337;
  height: 160rpx;
  background-color: #ccc;
  border-radius: 10rpx;
}

.exclusive_right .txt {
  font-size: 26rpx;
  color: #03BF9D;
  line-height: 1;
  padding: 28rpx 0 16rpx 20rpx;
}

.exclusive_right .txt1 {
  margin-left: 20rpx;
}

.recommend_title {
  font-size: 32rpx;
  color: #010101;
  align-items: center;
  margin-left: 30rpx;
  margin-bottom: 16rpx;
  line-height: 32rpx;
}

.recommend_title .border_left {
  width: 4rpx;
  height: 24rpx;
  background: linear-gradient(360deg, #03BF9D 0%, #AEF7EA 100%);
}

/*goodsList start*/
.goods_list {
  width: 690rpx;
  margin: 0 auto;
}

.goods_list .item {

}

.goods_item {
  width: 337rpx;
}

.goods_list .goods_img {
  width: 337rpx;
  border-radius: 10rpx;
  background-color: #ccc;
  display: block;
}

.goods_item .item .txt {
  font-size: 28rpx;
  line-height: 36rpx;
  color: #333333;
  margin-top: 20rpx;
  /*max-width: 308rpx;*/
}

.goods_item .item .txt1 {
  line-height: 1;
  max-width: 220rpx;
}

.goods_item_bottom {
  font-size: 24rpx;
  color: #999;
  align-items: center;
  justify-content: space-between;
  margin-top: 13rpx;
}

.goods_item_bottom .avatar {
  width: 40rpx;
  height: 40rpx;
  background-color: #ccc;
  border-radius: 40rpx;
  margin-right: 8rpx;
}

.iconaixin {
  margin-right: 12rpx;
  font-size: 30rpx;
  font-weight: 600;
}

/*goodsList end*/

.mask_ad {
  position: fixed;
  top: 0;
  width: 750rpx;
  bottom: 0;
  background-color: rgba(0,0,0, .25);
}

.mask_ad_content {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 559rpx;
}

.mask_ad_content .top {
  width: 559rpx;
  height: 913rpx;
  background-color: #ccc;
  background-image: url("https://www.mengniuhealth.cn/fitment/icon/index_ad_bg.png");
  background-size: cover;
  background-repeat: no-repeat;
  border-radius: 20rpx;
}

.iconguanbi {
  font-size: 45rpx;
  color: #fff;
  text-align: center;
  padding-top: 24rpx;
}

.mask_ad_content .price_wrap {
  width: 560rpx;
  height: 400rpx;
}

.mask_ad_content .price {
  font-size: 30rpx;
  color: #FF5531;
  text-align: center;
  line-height: 1;
  padding-top: 23rpx;
  margin-bottom: 12rpx;
}

.mask_ad_content .price .num {
  font-size: 50rpx;
  line-height: 1;
}

.mask_ad_content .explain {
  font-size: 22rpx;
  line-height: 1;
  color: #8D434D;
  text-align: center;
}

.get_phone {
  width: 260rpx;
  height: 66rpx;
  background: #FFFFFF;
  border-radius: 34rpx;
  color: 333;
  font-size: 24rpx;
}

.get_phone1 {
  margin: 126rpx auto 25rpx;
  padding: 0 35rpx;
}

.phone_code {
  width: 330rpx;
  margin: 0 auto;
}

.phone_code input {
  width: 157rpx;
  height: 66rpx;
  padding-left: 35rpx;
}

.phone_code .code_btn {
  width: 139rpx;
  background: linear-gradient(180deg, #FBE6CF 0%, #FACFA0 100%);
  border-radius: 0 34rpx 34rpx 0;
  font-size: 22rpx;
  line-height: 66rpx;
  text-align: center;
  color: #D23023;
}

.packet_red {
  background-image: url("https://www.mengniuhealth.cn/fitment/icon/index_ad_btn.png");
  height: 74rpx;
  width: 458rpx;
  background-size: cover;
  background-repeat: no-repeat;
  margin: 110rpx auto 0;
  font-size: 31rpx;
  line-height: 74rpx;
  text-align: center;
  color: #D23023;
}
